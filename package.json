{
  "name": "nextjs-graphql-dogs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:init": "yarn prisma:pull && yarn prisma:generate && yarn prisma:seed",
    "prisma:pull": "npx prisma db pull",
    "prisma:seed": "npx prisma db seed",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate"
  },
  "prisma": {
    "schema": "./src/lib/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/lib/prisma/seed.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "console.log('husky pre-push hook')"
    }
  },
  "lint-staged": {
    "*.(ts|tsx|js|jsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/server": "^4.5.0",
    "@as-integrations/next": "^1.2.0",
    "@prisma/client": "^4.11.0",
    "@types/node": "18.15.12",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4",
    "wb-private-api": "^0.4.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1"
  }
}
